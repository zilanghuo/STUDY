<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdmoney.message.log.dao.LogDataDAO">
    <resultMap type="com.zdmoney.message.log.model.LogData" id="getMap">
    </resultMap>

    <delete id="removeLogData">
        DELETE FROM T_LOG_DATA WHERE CREATE_TIME BETWEEN #{startTime} AND #{endTime};
    </delete>

</mapper>

