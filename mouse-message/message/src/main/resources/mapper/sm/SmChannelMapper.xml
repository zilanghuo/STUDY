<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdmoney.message.sm.dao.SmChannelDAO">
    <resultMap type="com.zdmoney.message.sm.model.SmChannel" id="getMap">

    </resultMap>

    <update id="updateStatus">
        update T_SM_CHANNEL t
        set t.status = #{status}
        where t.status = #{oldStatus}
        and t.no in
        <foreach item="item" index="index" collection="names" open="(" separator="," close=")">
            #{item}
        </foreach>
    </update>

</mapper>

